{
    "manifest_version": 3,
    "name": "Text Faker",
    "version": "1.0",
    "description": "Fake text of elements found by XPath when the page loads.",
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}